{
 "display_name": "PySpark",
 "language": "python",
 "argv": [
  "/usr/bin/python",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "SPARK_HOME": "/opt/spark/",
  "PYTHONPATH": "/opt/spark/python/",
  "PYTHONSTARTUP": "/opt/spark/python/pyspark/shell.py",
  "PYSPARK_SUBMIT_ARGS": "--master {{ jupyter_spark_master }} --num-executors {{ jupyter_spark_executors }} --conf spark.executor.cores={{ jupyter_spark_executor_cores }} --conf spark.executor.memory={{ jupyter_spark_executor_memory }} pyspark-shell"
 }
}
